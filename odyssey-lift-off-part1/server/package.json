  },
  "author": "Raphael Terrier @R4ph-t",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@graphql-tools/mock": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "graphql": "^16.8.0",
    "graphql-tag": "^2.12.6"
  }
}
