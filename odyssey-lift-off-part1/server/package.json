  "description": "back-end demo app for Apollo's lift-off course",
  "main": "src/index.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "compile": "tsc",
    "dev": "ts-node-dev --respawn ./src/index.ts",
